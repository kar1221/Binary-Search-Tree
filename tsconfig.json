{
  "compilerOptions": {
      "lib": ["ESNext", "DOM"],
      "strict": true,
      "noUnusedParameters": true,
      "noFallthroughCasesInSwitch": true,
      "noUnusedLocals": true,
      "allowImportingTsExtensions": true,
      "noEmit": true,
      "esModuleInterop": true,
      "isolatedModules": true,
      "target": "ESNext",
      "module": "ESNext",
      "moduleResolution": "Node",
      "strictNullChecks": true
  }
}